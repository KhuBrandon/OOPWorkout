#include <iostream>      
using namespace std; 
void main() {
	//ax+by=c & dx+ey=f
	float a, b, c, d, e, f;
	cout << "Enter Floats : " << endl;
	cout << "a : "; cin >> a;
	cout << "b : "; cin >> b;
	cout << "c : "; cin >> c;
	cout << "d : "; cin >> d;
	cout << "e : "; cin >> e;
	cout << "f : "; cin >> f;
	cout << "X = " << (c * e - b * f) / (a * e - b * d) << endl;
	cout << "Y = " << (c * d - a * f) / (b * d - a * c) << endl;
}
